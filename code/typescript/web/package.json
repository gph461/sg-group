{
  "name": "vue-antdesign-typescript-starter",
  "version": "0.0.1",
  "description": "An Ant Design Project",
  "productName": "vue-antdesign-typescript-starter",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "serve": "pnpm i && vite",
    "serve:prod": "pnpm i && vite --mode production",
    "emulator": "npx ngx-http-server --rootFile dist/spa/index.html -p 9000",
    "build": "vite build --mode development",
    "build:prod": "vite build --mode production"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@vueuse/core": "^9.13.0",
    "ant-design-vue": "^4.2.1",
    "antd": "^5.19.2",
    "axios": "^1.6.2",
    "change-case": "^4.1.2",
    "dayjs": "^1.11.10",
    "libphonenumber-js": "^1.10.52",
    "mime-types": "^2.1.35",
    "notivue": "^2.4.4",
    "pinia": "^2.1.7",
    "vue": "^3.3.13",
    "vue-country-flag-next": "^2.3.2",
    "vue-request": "^2.0.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^18.19.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-typescript": "^11.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.19.2",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "sass": "1.33.0",
    "sass-loader": "^14.2.1",
    "typescript": "^4.9.5",
    "unimport": "^3.7.1",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^4.5.1",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-top-level-await": "^1.4.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}